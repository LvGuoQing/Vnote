<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.cqu.vnote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.0" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="cn.edu.cqu.vnote.Activity.BucketHomeFragmentActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name="cn.edu.cqu.vnote.Activity.HomeFragmentActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name="cn.edu.cqu.vnote.StartActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>